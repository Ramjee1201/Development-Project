{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "WriteTo": [
    {
      "Name": "Console"
    },
    {
      "Name": "File",
      "Args": {
        "path": "C:/ErrorLogging/Savvy/LogFiles/Savvy-.json",
        "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
        "rollingInterval": "Day"
      },
      "restrictedToMinimumLevel": "Error"
    },
    {
      "Name": "MSSqlServer",
      "Args": {
        "connectionString": "Server=localhost;Database=Instance;User ID=Spark_User;Password=Spark@#;Trusted_Connection=False;MultipleActiveResultSets=true",
        "sinkOptionsSection": {
          "tableName": "ErrorLog",
          "schemaName": "Instance",
          "autoCreateSqlTable": true
        },
        "restrictedToMinimumLevel": "Error"
      }
    }
  ],
  "ConnectionStrings": {
    "APIContext": "Server=localhost;Database=Instance;User ID=Spark_User;Password=Spark@#;Trusted_Connection=False;MultipleActiveResultSets=true"
  },
  "AllowedHosts": "*"
}
